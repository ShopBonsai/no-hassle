{
  "name": "no-hassle",
  "version": "1.0.1",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "start": "npm run build && node build/index.js",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts --forceExit",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/source/cli.js --no-cleanup",
    "git:clean": "git branch --merged | egrep -v \"(^\\*|master|staging|develop)\" | xargs git branch -d",
    "git:clean-remote": "git remote prune origin"
  },
  "precommit": [
    "lint",
    "build"
  ],
  "dependencies": {
    "@hapi/joi": "15.1.1",
    "celebrate": "11.0.1",
    "express": "4.17.1",
    "joi-extract-type": "15.0.2",
    "joi-to-swagger": "git://github.com/ShopBonsai/joi-to-swagger.git",
    "swagger-jsdoc": "3.5.0",
    "swagger-ui-express": "4.1.2",
    "swagger2openapi": "5.3.1"
  },
  "devDependencies": {
    "@types/express": "4.17.2",
    "@types/jest": "^24.0.25",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0",
    "joi": "^14.3.1",
    "pre-commit": "^1.2.2",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "directories": {
    "test": "tests"
  }
}
